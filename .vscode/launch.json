{
    "workbench.colorCustomizations": {},
    "[oraclesql]": {
        "editor.suggest.showSnippets": true,
        "editor.quickSuggestions": {
            "comments": "on",
            "strings": "on",
            "other": "on"
        }
    },
    "editor.accessibilitySupport": "on",
    "redhat.telemetry.enabled": true,
    "editor.defaultColorDecorators": true,
    "csharp.format.enable": false,
    "csharp.semanticHighlighting.enabled": false,
    "razor.format.enable": false,
    "http.experimental.systemCertificatesV2": true,
    "workbench.editor.scrollToSwitchTabs": true,
    "workbench.editor.autoLockGroups": {
        "default": true
    },
    "gitlens.terminalLinks.enabled": false,
    "gitlens.views.contributors.showAllBranches": true,
    "gitlens.views.repositories.showWorktrees": false,
    "gitlens.views.worktrees.pullRequests.enabled": false,
    "gitlens.views.worktrees.pullRequests.showForBranches": false,
    "gitlens.views.worktrees.pullRequests.showForCommits": false,
    "gitlens.views.worktrees.files.compact": false,
    "gitlens.views.worktrees.avatars": false,
    "gitlens.views.worktrees.reveal": false,
    "gitlens.autolinks": [
        {
            "ignoreCase": false,
            "alphanumeric": false
        }
    ],
    "security.workspace.trust.emptyWindow": false,
    "security.workspace.trust.enabled": false,
    "githubIssues.queries": [
        {
            "label": "My Issues",
            "query": "default"
        },
        {
            "label": "Created Issues",
            "query": "author:${user} state:open repo:${owner}/${repository} sort:created-desc"
        },
        {
            "label": "Recent Issues",
            "query": "state:open repo:${owner}/${repository} sort:updated-desc"
        }
    ]
}
